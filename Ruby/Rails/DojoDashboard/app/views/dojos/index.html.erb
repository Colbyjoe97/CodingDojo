<div class="wrapper">
    <h1 class="text-center">Listing <%= session[:count] %> Dojos</h1>
    <div class="tableWrapper">
        <table class="table table-hover">
            <thead>
                <tr>
                    <td>Branch</td>
                    <td>Street</td>
                    <td>City</td>
                    <td>State</td>
                    <td>Actions</td>
                </tr>
            </thead>
            <tbody>
                <% @dojos.each do |dojo| %>
                <tr>
                    <td><%= dojo.branch %></td>
                    <td><%= dojo.street %></td>
                    <td><%= dojo.city %></td>
                    <td><%= dojo.state %></td>
                    <td>
                        <a href="<%= dojo_path dojo.id %>">View</a> |
                        <a href="<%= edit_dojo_path dojo.id %>">Edit</a> |
                        <a href="/dojos/<%=dojo.id%>" data-method="delete">Delete</a>
                    </td>
                </tr>
                <% end %>
            </tbody>
        </table>
    </div>
    <a href="<%= new_dojo_path %>"><p class="text-center">List New Dojo</p></a>
</div>